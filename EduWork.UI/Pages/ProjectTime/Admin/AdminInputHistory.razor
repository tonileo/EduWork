@page "/adminInputHistory"
@using EduWork.UI.Components.ProjectTime

<PageTitle>@ADMIN_INPUT_HISTORY_TITLE</PageTitle>

<CascadingValue Value="@isAdmin">
    <InputHistoryComponent ApiEndpoint=@ADMIN_INPUT_HISTORY_ENDPOINT></InputHistoryComponent>
</CascadingValue>

@code {
    public bool isAdmin { get; set; } = true;

    private const string ADMIN_INPUT_HISTORY_TITLE = "Admin povijest unosa";
    private const string ADMIN_INPUT_HISTORY_ENDPOINT = "api/ProjectTimes/historyFilter/admin";
}